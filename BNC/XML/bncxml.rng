    </optional>
    <ref name="att.rendered.attribute.rend"/>
    <optional>
      <attribute name="resp">
        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">a code identifying the agency responsible for making the
correction.</a:documentation>
