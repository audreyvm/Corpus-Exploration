        <text/>
      </attribute>
    </optional>
    <optional>
      <attribute name="reason">
        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">gives further details of the reason for omission.</a:documentation>
        <text/>
      </attribute>
