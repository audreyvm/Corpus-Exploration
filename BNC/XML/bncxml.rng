  </define>
  <define xmlns="http://relaxng.org/ns/structure/1.0" name="list.content">
    <group>
      <zeroOrMore>
        <choice>
          <ref name="model.divWrapper"/>
          <ref name="model.global"/>
        </choice>
      </zeroOrMore>
      <choice>
        <oneOrMore>
          <ref name="item"/>
          <zeroOrMore>
            <ref name="model.global"/>
          </zeroOrMore>
        </oneOrMore>
        <oneOrMore>
          <ref name="label"/>
          <zeroOrMore>
            <ref name="model.global"/>
          </zeroOrMore>
          <ref name="item"/>
          <zeroOrMore>
            <ref name="model.global"/>
