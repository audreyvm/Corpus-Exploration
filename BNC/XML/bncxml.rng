      <ref name="editorialDecl"/>
    </zeroOrMore>
    <zeroOrMore>
      <ref name="tagsDecl"/>
    </zeroOrMore>
    <zeroOrMore>
      <ref name="refsDecl"/>
    </zeroOrMore>
    <zeroOrMore>
      <ref name="classDecl"/>
    </zeroOrMore>
    <zeroOrMore>
      <ref name="xairaSpecification"/>
    </zeroOrMore>
  </define>
  <define xmlns="http://relaxng.org/ns/structure/1.0" name="model.encodingPart_sequenceRepeatable">
    <oneOrMore>
      <ref name="projectDesc"/>
    </oneOrMore>
    <oneOrMore>
      <ref name="samplingDecl"/>
    </oneOrMore>
    <oneOrMore>
      <ref name="editorialDecl"/>
    </oneOrMore>
    <oneOrMore>
      <ref name="tagsDecl"/>
    </oneOrMore>
    <oneOrMore>
