      <ref name="projectDesc"/>
    </optional>
    <optional>
      <ref name="samplingDecl"/>
    </optional>
    <optional>
      <ref name="editorialDecl"/>
    </optional>
    <optional>
      <ref name="tagsDecl"/>
    </optional>
    <optional>
      <ref name="refsDecl"/>
    </optional>
    <optional>
      <ref name="classDecl"/>
    </optional>
    <optional>
      <ref name="xairaSpecification"/>
    </optional>
  </define>
  <define xmlns="http://relaxng.org/ns/structure/1.0" name="model.encodingPart_sequenceOptionalRepeatable">
