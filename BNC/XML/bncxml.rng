  <define xmlns="http://relaxng.org/ns/structure/1.0" name="corr.attributes">
    <optional>
      <attribute name="sic">
        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">contains verbatim text which has been corrected, or an empty string if
the correction consists of an addition.</a:documentation>
